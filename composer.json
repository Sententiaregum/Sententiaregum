{
    "name": "sententiaregum/sententiaregum",
    "license": "GPL-2.0",
    "type": "project",
    "description": "A social network based on Symfony and ReactJS",
    "autoload": {
        "psr-4": { "AppBundle\\": "src/AppBundle/" }
    },
    "require": {
        "php": "^7.0",
        "symfony/symfony": "^3.1",
        "doctrine/orm": "^2.5",
        "doctrine/doctrine-bundle": "^1.6",
        "symfony/swiftmailer-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.11",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0",
        "incenteev/composer-parameter-handler": "^2.1",
        "wjzijderveld/check-bundles": "^1.1",
        "ma27/api-key-authentication-bundle": "^1.1",
        "doctrine/doctrine-fixtures-bundle": "^2.3",
        "twig/extensions": "^1.3",
        "friendsofsymfony/rest-bundle": "^2.0",
        "jms/serializer-bundle": "^1.1",
        "snc/redis-bundle": "^2.0",
        "predis/predis": "^1.0",
        "nelmio/api-doc-bundle": "^2.13",
        "doctrine/doctrine-migrations-bundle": "^1.1",
        "ramsey/uuid": "^3.4",
        "eightpoints/guzzle-bundle": "^4.5",
        "ocramius/proxy-manager": "^2.0",
        "roave/security-advisories": "dev-master",
        "doctrine/data-fixtures": "^1.1,<1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "5.3.*",
        "friendsofphp/php-cs-fixer": "^1.11",
        "behat/behat": "^3.1",
        "behat/symfony2-extension": "^2.1",
        "beberlei/assert": "^2.5",
        "malukenho/kawaii-gherkin": "^0.1.3",
        "phploc/phploc": "^3.0",
        "sllh/php-cs-fixer-styleci-bridge": "^1.6"
    },
    "scripts": {
        "post-install-cmd": [
            "WillemJan\\CheckBundles\\Composer\\CheckBundles::postPackageUpdate",
            "AppBundle\\Composer\\ScriptHandler::installNpmDependencies",
            "AppBundle\\Composer\\ScriptHandler::buildFrontendData",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "AppBundle\\Composer\\ScriptHandler::createDoctrineSchema"
        ],
        "post-update-cmd": [
            "WillemJan\\CheckBundles\\Composer\\CheckBundles::postPackageUpdate",
            "AppBundle\\Composer\\ScriptHandler::installNpmDependencies",
            "AppBundle\\Composer\\ScriptHandler::buildFrontendData",
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget",
            "AppBundle\\Composer\\ScriptHandler::createDoctrineSchema"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "checkbundles-ignore": [
            "Sensio\\Bundle\\DistributionBundle\\SensioDistributionBundle",
            "Sensio\\Bundle\\GeneratorBundle\\SensioGeneratorBundle",
            "Symfony\\Bundle\\DebugBundle\\DebugBundle",
            "Symfony\\Bundle\\WebProfilerBundle\\WebProfilerBundle"
        ],
        "checkbundles-kernels": {
            "AppKernel": "prod"
        }
    },
    "authors": [
        {
            "name": "Maximilian Bosch",
            "email": "maximilian.bosch.27@gmail.com",
            "homepage": "https://github.com/Ma27",
            "role": "Project Lead"
        },
        {
            "name": "Ben Bieler",
            "email": "benjaminbieler2014@gmail.com",
            "homepage": "https://github.com/benbieler",
            "role": "Core Developer"
        }
    ],
    "minimum-stability": "stable"
}
